{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksConfig": {
      "providers": ["php"],
      "phpPackages": [
        "php82Extensions.gd"
      ]
    }
  },
  "deploy": {
    "releaseCommand": "php artisan storage:link && php artisan migrate --force",
    "startCommand": "php artisan serve --host=0.0.0.0 --port=$PORT"
  }
}